@model Web.Mobile.Models.SponsorshipOverview

@{
    ViewBag.Title = "SponsorshipOverview";
}
@{
    var firstSponsoredDev = Model.SponsoredDevelopers.FirstOrDefault();
  }
@{
    if (Model.SelectedDeveloper != null)
    {
        <div id="devMiniProfile" style=" float:none; clear:right; max-height:100%; max-width:100%; overflow:hidden;">
            <h2>@Model.SelectedDeveloper.DeveloperName</h2>
            <img src="@Model.SelectedDeveloper.ImageUrl" alt="@Model.SelectedDeveloper.DeveloperName + image" width="40%" height="100%" style="float:left; margin-right: 10px;" />
            @Model.SelectedDeveloper.Bio
        </div>
        if (Model.SponsoredDevelopers.Count > 1)
        {
            <div id="devSelector" style="padding-top:5px; padding-bottom:5px; margin-top:5px; margin-bottom:5px; border-style:dashed; background-color:Gray; ">
                @foreach (var dev in Model.SponsoredDevelopers)
                {
                    <img src="@Url.Content(@dev.ImageUrl)" alt="@dev.DeveloperName" onclick='$.mobile.changePage("@Url.Action("DeveloperView", "Sponsor", new { developerName = dev.DeveloperName })", { transition: "slideup" })'/>
                }
            </div>       
        }
    }
    
}
<p>
    @Html.ActionLink("Edit", "Edit", new { /* id=Model.PrimaryKey */ }) |
    @Html.ActionLink("Back to List", "Index")
</p>
